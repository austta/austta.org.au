#!/usr/bin/env bash
set -o errexit
set -o pipefail
set -o nounset
# set -o xtrace

# Make the generated .html files have the same time as the source
# for the moment
for THIS_FILE in *.md
   do
     # Look for a generated file
     if [ -f "_site/${THIS_FILE/%.md/.html}" ]
      then
        touch -r "$THIS_FILE" "_site/${THIS_FILE/%.md/.html}"
      fi
  done
# Make the Table Talk epubs
for TABLE_TALK in 201601 201602 201603 201604 201605
  do
    pandoc --ascii -o _site/ttalk${TABLE_TALK}.epub ttalk${TABLE_TALK}.md
    touch -r ttalk${TABLE_TALK}.md _site/ttalk${TABLE_TALK}.epub
  done
# copy in and rename PDFs
# The distribtuion lists
cp -ip ../mailouts/"ATA DL Jul16.pdf" _site/"Distribution List 201607.pdf"
cp -ip ../mailouts/"ATA DL Aug16.pdf" _site/"Distribution List 201608.pdf"
# The auction catalogues
cp -ip ../mailouts/"ATA Auction 80 Catalogue Aug16.pdf" \
            _site/"Auction 80 Catalogue Aug 16.pdf"
cp -ip ../mailouts/"ATA Auction 79 Catalogue Jul16.pdf" \
            _site/"Auction 79 Catalogue Jul 16.pdf"
# Back issues of Table Talk
cp -ip ../mailouts/"ATA 2015 November Table Talk.pdf" \
            _site/"ATA Table Talk 201511 November.pdf"
cp -ip ../mailouts/"ATA 2015 December Table Talk.pdf" \
            _site/"ATA Table Talk 201512 December.pdf"
cp -ip ../mailouts/"ATA 2016 January Table talk.pdf" \
            _site/"ATA Table Talk 201601 January.pdf"
cp -ip ../mailouts/"ATA 2016 February Table Talk.pdf" \
            _site/"ATA Table Talk 201602 February.pdf"
cp -ip ../mailouts/"2016 March ATA Table Talk.pdf" \
            _site/"ATA Table Talk 201603 March.pdf"
cp -ip ../mailouts/"ATA 2016 April Table Talk.pdf" \
            _site/"ATA Table Talk 201604 April.pdf"
cp -ip ../mailouts/"ATA 2016 May Table Talk.pdf" \
            _site/"ATA Table Talk 201605 May.pdf"
# Back issues of The Times
cp -ip ../mailouts/"1604 The Times  April 2016a.pdf"\
            _site/"The Times 201604 April.pdf"
cp -ip ../mailouts/"1605 The Times May 2016 388 Std.pdf"\
            _site/"The Times 201605 May.pdf"
# The renewal form
cp -ip ../mailouts/"ATA 2016-17 Renewal form blank.pdf" \
            _site/"ATA 20162017 Renewal Form.pdf"
# The automated renewal form
cp -ip ../mailouts/Membershipform.pdf _site
# bulk load of old PDFs and Gifs etc
while IFS= read -r line; do
  if [ ! -f _site/"$line" ]
    then
      cp -ip ../old_site/"$line" _site
    else
      echo New file $line already exists! >&2
  fi
done <<END_OLD
2014 April.pdf
